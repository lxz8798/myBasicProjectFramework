{
  "name": "lazy-man-vue",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode development --progress --color",
    "build": "vue-cli-service build --mode production --progress --color",
    "build:pro": "vue-cli-service build --mode production --progress --color & xcopy /fy \"./config/index.js\" \"dist/config.js\"",
    "demo:demo": "vue-cli-service build --mode production --progress --color & xcopy /fy \"./config/demo.js\" \"dist/config.js\"",
    "test": "vue-cli-service serve --mode test",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "amfe-flexible": "^2.2.1",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "echarts": "^4.8.0",
    "glob": "^7.1.3",
    "iview": "^3.1.3",
    "lib-flexible": "^0.3.2",
    "lodash": "^4.17.11",
    "node-sass": "^4.9.4",
    "normalize.css": "^8.0.0",
    "postcss-pxtorem": "^5.1.1",
    "register-service-worker": "^1.7.1",
    "sass-loader": "7.1.0",
    "vant": "^1.6.17",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-e2e-cypress": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-plugin-pwa": "~4.4.0",
    "@vue/cli-plugin-router": "~4.4.0",
    "@vue/cli-plugin-unit-jest": "~4.4.0",
    "@vue/cli-plugin-vuex": "~4.4.0",
    "@vue/cli-service": "^4.4.6",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.0.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^23.0.1",
    "babel-plugin-import": "^1.11.0",
    "chai": "^4.1.2",
    "compass-mixins": "^0.12.10",
    "compression-webpack-plugin": "^4.0.0",
    "coveralls": "^3.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "html-webpack-plugin": "^3.2.0",
    "istanbul": "^0.4.5",
    "mockjs": "^1.1.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.19.1",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "sass-loader": "^7.1.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "vue-template-compiler": "^2.6.11",
    "vue-test-utils": "^1.0.0-beta.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "rules": {
      "generator-star-spacing": "off",
      "no-unused-vars": "off",
      "quotes": "off"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
